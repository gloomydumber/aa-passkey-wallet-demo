{
  "name": "aa-passkey-wallet",
  "version": "0.1.0",
  "private": true,
  "description": "AA Passkey Web Wallet - Account Abstraction with Passkey authentication",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@aa-wallet/web",
    "build": "npm run build --workspaces --if-present",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rimraf node_modules",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wallet",
    "ethereum",
    "account-abstraction",
    "eip-4337",
    "passkey",
    "webauthn"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4"
  }
}
